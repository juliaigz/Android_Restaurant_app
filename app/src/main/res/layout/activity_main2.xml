<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <RatingBar
        android:id="@+id/ratingBar2"
        android:layout_width="231dp"
        android:layout_height="43dp"
        android:layout_marginEnd="6dp"
        android:contentDescription="rating bar"
        android:scaleX="0.5"
        android:scaleY="0.5"
        android:transformPivotX="@android:dimen/thumbnail_width"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2" />

    <RatingBar
        android:id="@+id/ratingBar3"
        android:layout_width="231dp"
        android:layout_height="43dp"
        android:layout_marginEnd="10dp"
        android:contentDescription="rating bar"
        android:scaleX="0.5"
        android:scaleY="0.5"
        android:transformPivotX="@android:dimen/thumbnail_width"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="@color/design_default_color_error"
        android:contentDescription="barra"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="LUNCH"
        android:textColor="@color/design_default_color_background"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="405dp"
        android:layout_height="169dp"
        android:layout_marginStart="3dp"
        android:contentDescription="comida 1"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:srcCompat="@drawable/img_1"
        tools:srcCompat="@drawable/img_1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="192dp"
        android:layout_height="19dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="176dp"
        android:text="Rice with Daal Godhi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageButton2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="192dp"
        android:layout_height="19dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="142dp"
        android:text="Rice with Daal Godhi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageButton3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="192dp"
        android:layout_height="19dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="186dp"
        android:text="Rice with Daal Godhi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageButton1" />

    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="231dp"
        android:layout_height="43dp"
        android:layout_marginEnd="3dp"
        android:contentDescription="rating bar"
        android:scaleX="0.5"
        android:scaleY="0.5"
        android:transformPivotX="@android:dimen/thumbnail_width"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton1" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="389dp"
        android:layout_height="165dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="9dp"
        android:contentDescription="comida2"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ratingBar"
        app:srcCompat="@drawable/img_2"
        tools:srcCompat="@drawable/img_2" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="393dp"
        android:layout_height="130dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="65dp"
        android:contentDescription="3"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ratingBar2"
        app:srcCompat="@drawable/img_3" />

</androidx.constraintlayout.widget.ConstraintLayout>